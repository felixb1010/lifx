{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "lifx",
  "title": "LIFX",
  "description": "Control your lifx lights with this extension",
  "icon": "command-icon.png",
  "author": "felixb1010",
  "categories": [
    "Fun",
    "Applications",
    "Other",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Turn on light",
      "subtitle": "Toggle your main LIFX light",
      "description": "Turns on the light to bright white",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.38.3",
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
